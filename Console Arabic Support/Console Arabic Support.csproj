<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	<TargetFrameworks>net40;net45;netcoreapp3.1</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Authors>SoldierPePe</Authors>
    <PackageId>PePe.ConsoleArabicSupport</PackageId>
    <Description>Library to to allow your console app support Arabic language</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/SoldierPePe/Console-Arabic-Support</RepositoryUrl>
    <RepositoryType></RepositoryType>
    <PackageTags>Console, Arabic, Text</PackageTags>
  </PropertyGroup>
</Project>
